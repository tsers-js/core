{
  "name": "tsers",
  "version": "2.0.0-rc0",
  "private": false,
  "description": "Transducer-Signal-Executor framework for Reactive Streams",
  "main": "dist/tsers.core.js",
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsers-js/core.git"
  },
  "keywords": [
    "rxjs",
    "reactive",
    "TSERS"
  ],
  "scripts": {
    "postinstall": "npm run init",
    "init": "lerna bootstrap && lerna run update:binaries",
    "create:package": "lehm create package",
    "test": "lerna run test",
    "test:ci": "lerna run test:ci",
    "report:ci": "lerna run report:ci",
    "postreport:ci": "rimraf coverage && mkdir -p coverage && lcov-result-merger 'packages/*/coverage/lcov.info' coverage/lcov.info"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "codecov": "^1.0.1",
    "eslint": "^3.3.1",
    "lcov-result-merger": "^1.2.0",
    "lehm": "^0.1.0",
    "lerna": "^2.0.0-beta.24",
    "mocha": "^2.5.3",
    "npm-check-updates": "^2.7.2",
    "nyc": "^7.1.0",
    "rimraf": "^2.5.3",
    "rollup": "^0.34.1",
    "rollup-plugin-buble": "^0.12.1",
    "rollup-plugin-node-resolve": "^1.7.1",
    "shelljs": "^0.7.3",
    "uglify-js": "^2.7.0"
  }
}
